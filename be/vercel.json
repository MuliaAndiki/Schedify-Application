{
  "version": 2,
  "builds": [
    {
      "src": "src/serve.ts",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs20.x"
      }
    }
  ],
  "functions": {
    "src/serve.ts": {
      "runtime": "nodejs20.x",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/serve.ts"
    }
  ]
}
